{
 "manifest_version": 3,
 "name": "GitLab Resolved",
 "version": "1.0.3",
 "description": "Provides a small feature for hiding resolved threads in a merge request.",
 "short_name": "GitLab Resolved",
 "permissions": [],
 "content_scripts": [
   {
     "matches": ["https://*/*/-/merge_requests/*"],
     "css": ["main.css"],
     "js": ["main.js"]
   }
 ],
 "icons": {
   "16": "icons/gitlab-resolve-16.png",
   "48": "icons/gitlab-resolve-48.png",
   "128": "icons/gitlab-resolve-128.png"
 }
}